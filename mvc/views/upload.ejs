<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title></title>
  <link rel="stylesheet" href="/static/css/css.css">
  <link rel="stylesheet" href="/static/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="/static/css/jquery-confirm.css" />
  <style>
  .jconfirm.jconfirm-white .jconfirm-bg {background-color: rgba(0, 0, 0, 1);}
  </style>
  <script src="/static/js/jquery-1.11.0.min.js"></script>
  <script src="/static/js/jquery-confirm.js"></script>
</head>
<body>
<%-include("menu",{folder})%>
  <div class="jumbotron col-md-6 col-md-offset-3 well">
    <div class="container">

    <div class="uploadbar">
      <div class="col-md-6">当前目录：<b class="bigtxt">/uploads/<%=folder%></b> </div>
      <div class="col-md-6 right text-right"><a class="btn btn-default btn-sm right" href="/photos/<%=folder%>">返回相册</a></div>
      </div>
    </div>   

    <hr/>

    <form class="form-group myform" action="/upload/<%=folder%>" enctype="multipart/form-data" method="post">
          <div class="alert alert-info col-md-6 col-md-offset-3">
            <input  type="file" name="tupian" id="tupian" multiple="multiple"></div>
          <br/>
          <div class="col-md-12 text-center">
          <input type="submit"  value="开始上传" class="btn btn-success btn-lg">
        </div>
    </form>

    <script>
    function StartUpLoad(){
      if($("#tupian").val()!=''){
          $('.myform').submit();
          return true;
      }else{
        return false;
      }
    }
    </script>

    </div>
  </div>
</body>
</html>